{
  "manifest_version": 3,
  "name": "Clay Enrichment",
  "version": "1.1",
  "description": "Capture URLs and enrich them with Clay data",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "ThekilnLogoRounded.png",
      "48": "ThekilnLogoRounded.png",
      "128": "ThekilnLogoRounded.png"
    }
  },
  "icons": {
    "16": "ThekilnLogoRounded.png",
    "48": "ThekilnLogoRounded.png",
    "128": "ThekilnLogoRounded.png"
  }
}
